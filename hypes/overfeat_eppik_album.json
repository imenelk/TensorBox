{
    "data": {
        "train_idl": "./data/eppik/train_albums_boxes.json",
        "test_idl": "./data/eppik/val_albums_boxes.json",
        "truncate_data": false
    },
    "logging": {
        "display_iter": 50,
        "save_iter": 10000
    },
    "solver": {
        "opt": "RMS",
        "use_jitter": false,
        "rnd_seed": 1,
        "epsilon": 0.00001,
        "learning_rate": 0.001,
        "learning_rate_step": 33000,
        "hungarian_iou": 0.25,
        "weights": "",
        "head_weights": [1.0, 0.1]
    },
    "use_lstm": false,
    "use_rezoom": false,
    "rezoom_change_loss": "center",
    "reregress": false,
    "deconv": false,
    "image_width": 640,
    "image_height": 480,
    "region_size": 32,
    "num_classes": 2,
    "rnn_len": 1,
    "batch_size": 1,
    "slim_basename": "InceptionV1",
    "slim_top_lname": "Mixed_5b",
    "slim_attention_lname": "Mixed_3b",
    "slim_ckpt": "inception_v1.ckpt",
    "early_feat_channels": 256,
    "avg_pool_size": 5,
    "later_feat_channels": 832,
    "lstm_size": 500,
    "grayscale": true,
    "grayscale_prob": 0.8,

    "biggest_box_px": 10000,
    "focus_size": 1.8,
    "num_lstm_layers": 2,
    "clip_norm": 1.0,

}
